# Token du bot Discord (https://discord.com/developers/applications)
DISCORD_TOKEN=your_bot_token_here

# ID de l'application Discord
DISCORD_CLIENT_ID=your_client_id_here

# IDs des serveurs Discord, séparés par des virgules
DISCORD_GUILD_IDS=id_serveur_1,id_serveur_2

# ID du rôle admin F1 (optionnel, sinon seul le propriétaire du serveur peut administrer)
ADMIN_ROLE_ID=

# IDs des channels pour les annonces automatiques, séparés par des virgules
ANNOUNCE_CHANNEL_IDS=id_channel_1,id_channel_2

# Saison F1 en cours
F1_SEASON=2026
